<Window x:Class="CardsWPF.Views.AddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CardsWPF.Views"
        xmlns:m ="clr-namespace:CardsWPF.Models"
        mc:Ignorable="d"
        Title="AddWindow" Height="150" Width="150">
    <Window.DataContext>
        <m:Card>
        
        </m:Card>
    </Window.DataContext>
    <Grid>
        <TextBox x:Name="TitleBox" HorizontalAlignment="Center" Margin="0,0,0,70" Width="140" Height="35" />
        <Button Content ="Image" Click="AddFile_OnClick" HorizontalAlignment="Center" Margin="0,10,0,0" Width="100" Height="35"/>
        <Button Content ="Accept" Click ="Accept_OnClick" VerticalAlignment="Bottom" Margin="0,0,0,5" Width="100" Height="35"/>
    </Grid>
</Window>
